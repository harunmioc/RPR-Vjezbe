<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="310.0" maxWidth="500.0" minHeight="310.0" minWidth="500.0" prefHeight="310.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lv78.HelloController">
   <center>
      <HBox prefHeight="170.0" prefWidth="590.0" BorderPane.alignment="CENTER">
         <children>
            <ListView fx:id="listKorisnici" prefHeight="300.0" prefWidth="225.0">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="20.0" />
