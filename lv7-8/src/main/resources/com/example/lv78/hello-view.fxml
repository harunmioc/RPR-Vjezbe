                  </TextField>
                  <PasswordField fx:id="lozinkaField">
                     <VBox.margin>
                        <Insets bottom="5.0" left="10.0" top="5.0" />
                     </VBox.margin>
                  </PasswordField>
               </children>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="25.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </center>
   <bottom>

      <HBox alignment="TOP_RIGHT" prefHeight="53.0" prefWidth="490.0">
         <children>
            <Button fx:id="buttonDodaj" alignment="CENTER_RIGHT" minWidth="50.0" mnemonicParsing="false" onAction="#onDodajButtonClick" text="Dodaj">
